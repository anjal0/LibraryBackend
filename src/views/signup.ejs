<div class="container">
  <div class="row">
    
    <div class="col-md-12"> 
      
      <div class="panel">
        <div class="panel-body">
         
          
          
          <!--/stories-->
        <form action="/index" onsubmit="return (validate() && validpass() && passwordChanged())" class="frm">  
          <p>Please fill in this form to create an account.</p>
        <hr>
        <p>
        <label for="uname"><b>User Name</b></label>
        <input type="text" placeholder="Enter Username" id="uname" required >
        </p>
        <br>
        <br>
        <p>
        <label for="email"><b>Email</b></label>
        <input type="text" placeholder="Enter Email" id="email" required >
        </p>
        <br>
        <br>
        <label for="error" id="error"></label>
        <p>
        <label for="phone"><b>Password</b></label>
        <input name="password" id="psw" type="password" size="15" maxlength="100" onkeyup="return passwordChanged();" />
        </p>
        <span id="strength"><h5>Type Password</h5></span>
        <br>
        <br>
        <p>By creating an account you agree to our <a href="#" style="color:dodgerblue">Terms & Privacy</a>.</p>
    
        <div class="clearfix">
          <button type="button" class="cancelbtn">Cancel</button>
          <button type="submit" class="signupbtn">Sign Up</button>
        </div>
        </form>
          <!--/stories-->
          
          
          <a href="/" class="btn btn-primary pull-right btnNext">More <i class="glyphicon glyphicon-chevron-right"></i></a>
        
          
        </div>
      </div>
                                                                                       
	                                                
                                                      
   	</div><!--/col-12-->
  </div>
</div>
                                                
                                                                                
